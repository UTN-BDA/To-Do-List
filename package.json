{
  "name": "backend",
  "version": "1.0.0",
  "description": "proyecto con Express y TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd src/server && npm start",
    "client": "cd src/client && npm start",
    "test": "jest --detectOpenHandles",
    "test:coverage": "npm run pretest && jest --detectOpenHandles --coverage",
    "dev": "nodemon --exec ts-node src/index.ts",
    "pretest": "ts-node ./src/data --clear",
    "copy": "COPY . /app"
  },
  "author": "eliriper123@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "axios": "^1.9.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "typescript": "^4.0.0"
